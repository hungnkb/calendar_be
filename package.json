{
  "name": "calendar-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "dev": "rimraf dist && tsc-watch --noClear -p tsconfig.build.json --onSuccess \"node dist/server.js\"",
    "typeorm": "npm run build && npx typeorm -d dist/src/configs/dbconfig.js",
    "migrate:create": "npm run typeorm -- migration:generate",
    "migrate:up": "npm run typeorm -- migration:run",
    "migrate:down": "npm run typeorm -- migration:revert",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^7.2.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "tsc-watch": "^6.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
